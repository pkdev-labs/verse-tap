{
  "name": "pkdev/verse-client",
  "description": "Bible Verse Client developed by PkDev",
  "version": "1.0.0",
  "type": "library",
  "autoload": {
    "psr-4": {
      "PkDev\\VerseClient\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "license": "MIT",
  "autoload-dev": {
    "psr-4": {
      "PkDev\\VerseClient\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests",
    "test-coverage": "vendor/bin/phpunit tests --coverage-html coverage"
  },
  "keywords": ["bible", "verse"],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pkdev-labs/pkdev-traits"
    }
  ],
  "support": {
    "issues": "https://github.com/pkdev-labs/pkdev-traits/issues",
    "source": "https://github.com/pkdev-labs/pkdev-traits"
  }
}